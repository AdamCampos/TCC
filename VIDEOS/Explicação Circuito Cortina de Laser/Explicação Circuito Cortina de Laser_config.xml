      <rdf:Description tsc:version="2.0.1" dc:date="2017-03-18 06:20:43 " dc:source="Camtasia Studio,8.2.1,enu" dc:title="Explicação Circuito Cortina de Laser" tscDM:firstFrame="Explicação_Circuito_Cortina_de_Laser_First_Frame.png" tscDM:originId="15BCBE5F-FE49-4424-AF6A-1BE074126D23" tscDM:project="Explicação Circuito Cortina de Laser">
